$TTL 1D
@	IN SOA	@ admin.mtulio.eng.br. (
					1	; serial
					1D	; refresh
					1H	; retry
					1W	; expire
					3H )	; minimum

;--> Record Name Servers / Servidores de nomes

	NS	ns1.ict-eng.net.
@	NS	ns2.ict-eng.net.

;--> Record Address - zone
	A	200.200.200.1
	AAAA	::1

;--> Record Mail eXchange

@	MX 10	mx1.ict-eng.net.
@	MX 10	mx2.ict-eng.net.

;--> Record Address

ns1			IN A	200.200.200.1
ns2.ict-eng.net.	IN A	200.200.200.2
mx1			IN A	200.200.200.3
			IN A	200.200.200.4
mx2.ict-eng.net.	IN A	200.200.200.5
webserver.ict-eng.net.	IN A	200.200.200.10

;--> Records CNAME / Alias
www			CNAME	webserver
wiki			CNAME	webserver.ict-eng.net.
portal.ict-eng.net.	CNAME	webserver.ict-eng.net.


